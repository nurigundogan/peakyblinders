---
fullnameOverride: {{ jenkins.deployment.name }}

controller:
  ingress:
    enabled: true
    annotations: {
      kubernetes.io/ingress.class: nginx
    }
    hostName: thomas.nurigundogan.net

persistence:
  enabled: true
  accessMode: ReadWriteOnce
  storageClass: default
  size: 8Gi

resources:
  requests:
    cpu: 0.5
    memory: 1Gi
  limits:
    cpu: 1
    memory: 2Gi
